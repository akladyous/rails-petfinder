<%= turbo_frame_tag "posts" do %>
  <div class="container text-center">
    <div class="row" id="search">
    </div>
    <div class="row row-cols-1 row-cols-md-3 row-cols-lg-3 g3">
      <% @listings_posts.each do |listing| %>
        <%= render 'post', listing: listing %>
      <% end %>
    </div>
  </div>
<% end %>
